
{
    "env": {
        "browser": true,
        "commonjs": true,
        "node":true,
        "es6":true
    },
    "parserOptions": {
        "ecmaFeatures": {
          "experimentalObjectRestSpread": true
        },
        "sourceType": "module",
        "ecmaVersion": 6
      },
    "extends": "eslint:recommended",
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
      "no-extra-semi": 2,
      "indent": [2, 2],
      "no-debugger": 1,
      "no-console": [1, {"allow": ["error"]}],
      "no-case-declarations": 2,
      "no-empty": 2,
      "no-class-assign": 1,
      "no-unused-vars": 1
    }
}